      EditMask = '##/##/####'
    end
    object cdsResumoVendasCAIXA: TIntegerField
      DisplayLabel = 'Caixa'
      FieldName = 'CAIXA'
    end
    object cdsResumoVendasDESCRICAO_PAGAMENTO: TStringField
      DisplayLabel = 'Forma Pagmento'

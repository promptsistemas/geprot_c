  object qMateriaPrima: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'ID_ARTIGO'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'SELECT'
      ' A.ID_ARTIGO,'
      ' A.NOME_ARTIGO'
      'FROM TB_ARTIGO A'
      
        'WHERE NOT EXISTS(SELECT IP.ID_ARTIGO FROM TB_ITEM_PRECO_MALHARIA' +
        ' IP'
      
        '                 WHERE IP.ID_ARTIGO = A.ID_ARTIGO AND IP.ID_PREC' +
        'O_MALHARIA = :ID_ARTIGO)')
    SQLConnection = DM.conexao
    Left = 456
    Top = 284
    object qMateriaPrimaID_ARTIGO: TIntegerField
      FieldName = 'ID_ARTIGO'
      Required = True
    end
    object qMateriaPrimaNOME_ARTIGO: TStringField
      FieldName = 'NOME_ARTIGO'
      Size = 30
    end
  end

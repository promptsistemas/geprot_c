      'IA,'#13#10' F.SITUACAO,'#13#10' F.DT_ABERTURA,'#13#10' F.CNAE_PRINCIPAL,'#13#10' F.CNAE_' +
      'SECUNDARIO,'#13#10' F.ID_CATEGORIA,'#13#10' M.NOME AS CIDADE,'#13#10' M.UF AS UF,'#13 +
      #10' M.DDD,'#13#10' C.NOME_CATEGORIA'#13#10'FROM TB_FORNECEDOR F'#13#10'JOIN TB_MUNIC' +
      'IPIOS M ON (M.CODIGO_MUNIC = F.CODIGO_MUNIC)'#13#10'LEFT JOIN TB_CATEG' +
      'ORIA  C ON (C.ID_CATEGORIA = F.ID_CATEGORIA)'

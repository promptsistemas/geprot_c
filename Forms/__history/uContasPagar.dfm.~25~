  object qContasPagar: TSQLDataSet
    CommandText = 
      'SELECT'#13#10'    CP.ID_CONTAS_PAGAR    ,'#13#10'    CP.ID_PRODUCAO        ,' +
      #13#10'    CP.ID_CLIENTE         ,'#13#10'    CP.ID_PLANO_PAGAMENTO ,'#13#10'    ' +
      'CP.ID_USUARIO         ,'#13#10'    CP.ID_LOJA            ,'#13#10'    CP.ID_' +
      'FORNECEDOR,'#13#10'    CP.ID_FORN_REM,'#13#10'    CP.DT_EMISSAO         ,'#13#10' ' +
      '   CP.DT_VENCIMENTO      ,'#13#10'    CP.DT_PAGAMENTO       ,'#13#10'    CP.' +
      'QT_DIAS            ,'#13#10'    CP.QT_DIAS_ATRAZO     ,'#13#10'    CP.QT_PAR' +
      'CELA         ,'#13#10'    CP.NR_DOC             ,'#13#10'    CP.VL_COMPRA   ' +
      '       ,'#13#10'    CP.VL_PARCELA         ,'#13#10'    CP.NR_PARCELA        ' +
      ' ,'#13#10'    CP.STATUS             ,'#13#10'    CP.NR_NOTA_FISCAL,'#13#10'    CP.' +
      'NR_NF_REMESSA,'#13#10'    CP.TP_MOV,'#13#10'    PP.DESCRICAO_PAGAMENTO,'#13#10'   ' +
      ' C.R_SOCIAL AS CLIENTE ,'#13#10'    F.NOME AS FORNECEDOR  ,'#13#10'    FR.NO' +
      'ME AS FORN_REMESSA,'#13#10'    U.NOME_USUARIO         ,'#13#10'    L.R_SOCIA' +
      'L AS EMPRESA'#13#10'FROM TB_CONTAS_PAGAR CP'#13#10'JOIN TB_PLANO_PAGAMENTO P' +
      'P ON (PP.ID_PLANO_PAGAMENTO = CP.ID_PLANO_PAGAMENTO)'#13#10'JOIN TB_CL' +
      'IENTE C ON (C.ID_CLIENTE = CP.ID_CLIENTE)'#13#10'JOIN TB_FORNECEDOR F ' +
      'ON (F.ID_FORNECEDOR = CP.ID_FORNECEDOR)'#13#10'JOIN TB_FORNECEDOR FR O' +
      'N (FR.ID_FORNECEDOR = CP.ID_FORN_REM)'#13#10'JOIN TB_PRODUCAO P ON (P.' +
      'ID_PRODUCAO = CP.ID_PRODUCAO)'#13#10'JOIN TB_USUARIO U ON (U.ID_USUARI' +
      'O = CP.ID_USUARIO)'#13#10'JOIN TB_LOJAS L ON (L.ID_LOJA = CP.ID_LOJA)'#13 +
      #10'WHERE CP.ID_CLIENTE = :ID_CLIENTE'#13#10'AND CP.DT_EMISSAO   = :DT_EM' +
      'ISSAO'#13#10'AND CP.ID_PRODUCAO  = :ID_PRODUCAO'#13#10'ORDER BY CP.DT_VENCIM' +
      'ENTO'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'ID_CLIENTE'
        ParamType = ptInput
      end
      item
        DataType = ftDate
        Name = 'DT_EMISSAO'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'ID_PRODUCAO'
        ParamType = ptInput
      end>
    SQLConnection = DM.conexao

  object qFornecedor: TSQLDataSet
    CommandText = 
      'SELECT'#13#10' F.ID_FORNECEDOR,'#13#10' F.CODIGO_MUNIC,'#13#10' F.DT_CADASTRO,'#13#10' F' +
      '.NOME,'#13#10' F.TP_LOGRADOURO,'#13#10' F.ENDERECO,'#13#10' F.COMPLEMENTO,'#13#10' F.NUM' +
      'ERO,'#13#10' F.BAIRRO,'#13#10' F.CEP,'#13#10' F.TELFONE,'#13#10' F.FAX,'#13#10' F.CELULAR,'#13#10' F' +
      '.EMAIL,'#13#10' F.CNPJ,'#13#10' F.INS_EST,'#13#10' F.CONTATO,'#13#10' F.SITE,'#13#10' F.FANTAS' +
      'IA,'#13#10' F.SITUACAO,'#13#10' F.DT_ABERTURA,'#13#10' F.CNAE_PRINCIPAL,'#13#10' F.CNAE_' +
      'SECUNDARIO,'#13#10' F.ID_CATEGORIA,'#13#10' M.NOME AS CIDADE,'#13#10' M.UF AS UF,'#13 +
      #10' M.DDD,'#13#10' C.NOME_CATEGORIA'#13#10'FROM TB_FORNECEDOR F'#13#10'JOIN TB_MUNIC' +
      'IPIOS M ON (M.CODIGO_MUNIC = F.CODIGO_MUNIC)'#13#10'LEFT JOIN TB_CATEG' +
      'ORIA  C ON (C.ID_CATEGORIA = F.ID_CATEGORIA)'#13#10'WHERE F.ID_CATEGOR' +
      'IA = 2'
    MaxBlobSize = -1

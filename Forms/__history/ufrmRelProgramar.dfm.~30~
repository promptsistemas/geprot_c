  object dspProgramar: TDataSetProvider
    DataSet = qProgramar
    Options = [poIncFieldProps, poAllowMultiRecordUpdates, poAutoRefresh, poAllowCommandText, poUseQuoteChar]
    Left = 160
    Top = 217
  end
  object qProgramar: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'SELECT'
      ' F.NOME AS TINTURARIA,'
      ' M.NOME AS MALHARIA,'
      'MP.NOME_MATERIA as ARTIGO,'
      ' RP.NR_NF_REMESSA,'
      ' RP.DT_EMISSAO,'
      ' PRP.DT_EMISSAO AS DT_PROGRAMADO,'
      ' PRP.ID_FOR_TINTURARIA,'
      ' SUM(IR.QUANT_FATURADO) AS QUANT_NF,'
      '  ('
      '    SELECT SUM(IP.PESO) AS QUANT_PROGRAMADO'
      '    FROM TB_ITEM_PRG IP'
      
        '    JOIN TB_PRG_TINTURARIA PR   ON (PR.ID_PRG_TINTURARIA = IP.ID' +
        '_PRG_TINTURARIA)'
      '  WHERE PR.NR_NOTA_REMESSA= RP.NR_NF_REMESSA'
      '   GROUP BY PR.NR_NOTA_REMESSA, PR.DT_EMISSAO'
      '  ),'
      ''
      ' SUM(IR.QUANT_FATURADO) -'
      '  ('
      '    SELECT SUM(IP.PESO) AS QUANT_PROGRAMADO'
      '    FROM TB_ITEM_PRG IP'
      
        '    JOIN TB_PRG_TINTURARIA PR   ON (PR.ID_PRG_TINTURARIA = IP.ID' +
        '_PRG_TINTURARIA)'
      '  WHERE PR.NR_NOTA_REMESSA= RP.NR_NF_REMESSA'
      '   GROUP BY PR.NR_NOTA_REMESSA, PR.DT_EMISSAO'
      '  )AS QT_SEM_PROGR,'
      '  PRP.DT_EMISSAO AS PROGRAMADO,'
      ' C.R_SOCIAL'
      'FROM TB_ITEM_REMESSA_PRODUCAO IR'
      
        'LEFT OUTER JOIN TB_REMESSA_PRODUCAO RP ON (RP.ID_REMESSA = IR.ID' +
        '_REMESSA)'
      'LEFT OUTER JOIN TB_ARTIGO A ON (A.ID_ARTIGO = IR.ID_ARTIGO)'
      'LEFT OUTER JOIN TB_CLIENTE C ON (C.ID_CLIENTE = IR.ID_CLIENTE)'
      
        'LEFT OUTER JOIN TB_PRG_TINTURARIA PRP ON (PRP.NR_NOTA_REMESSA = ' +
        'RP.NR_NF_REMESSA)'
      
        'LEFT OUTER JOIN TB_FORNECEDOR F ON (F.ID_FORNECEDOR = PRP.ID_FOR' +
        '_TINTURARIA)'
      
        'LEFT OUTER JOIN TB_FORNECEDOR M ON (M.ID_FORNECEDOR = IR.ID_FORN' +
        '_MALHARIA)'
      
        'LEFT OUTER JOIN TB_MATERIA_PRIMA MP ON (MP.ID_MATERIA = IR.ID_MA' +
        'TERIA)'
      'WHERE C.ID_GRUPO_EMPRESA = 2'
      'AND PRP.ID_FOR_TINTURARIA = 7'
      'GROUP BY'
      ' M.NOME ,'
      ' MP.NOME_MATERIA,'
      ' RP.NR_NF_REMESSA,'
      ' RP.DT_EMISSAO,'
      ' C.R_SOCIAL,'
      ' PRP.DT_EMISSAO,'
      ' F.NOME,'
      'PRP.DT_EMISSAO,'
      ' PRP.ID_FOR_TINTURARIA')
    SQLConnection = DM.conexao
    Left = 128
    Top = 217
    object qProgramarTINTURARIA: TStringField
      FieldName = 'TINTURARIA'
      Size = 50
    end
    object qProgramarMALHARIA: TStringField
      FieldName = 'MALHARIA'
      Size = 50
    end
    object qProgramarNR_NF_REMESSA: TIntegerField
      FieldName = 'NR_NF_REMESSA'
    end
    object qProgramarDT_EMISSAO: TDateField
      FieldName = 'DT_EMISSAO'
    end
    object qProgramarQUANT_NF: TFMTBCDField
      FieldName = 'QUANT_NF'
      Precision = 18
      Size = 4
    end
    object qProgramarQUANT_PROGRAMADO: TFMTBCDField
      FieldName = 'QUANT_PROGRAMADO'
      Precision = 18
      Size = 3
    end
    object qProgramarQT_SEM_PROGR: TFMTBCDField
      FieldName = 'QT_SEM_PROGR'
      Precision = 18
      Size = 4
    end
    object qProgramarPROGRAMADO: TDateField
      FieldName = 'PROGRAMADO'
    end
    object qProgramarR_SOCIAL: TStringField
      FieldName = 'R_SOCIAL'
      Size = 50
    end
    object qProgramarARTIGO: TStringField
      FieldName = 'ARTIGO'
      Size = 30
    end
    object qProgramarDT_PROGRAMADO: TDateField
      FieldName = 'DT_PROGRAMADO'
    end
    object qProgramarID_FOR_TINTURARIA: TIntegerField
      FieldName = 'ID_FOR_TINTURARIA'
    end
  end

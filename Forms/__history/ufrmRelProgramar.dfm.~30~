      Size = 50
    end
    object cdsProgramarNR_NF_REMESSA: TIntegerField
      DisplayLabel = 'N'#186' Nota Fiscal'
      FieldName = 'NR_NF_REMESSA'
    end
    object cdsProgramarDT_EMISSAO: TDateField
      DisplayLabel = 'Dt Emiss'#227'o'
      FieldName = 'DT_EMISSAO'
      EditMask = '##/##/####'

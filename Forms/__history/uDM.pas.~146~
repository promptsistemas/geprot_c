{%CLASSGROUP 'Vcl.Controls.TControl'}

{$R *.dfm}

procedure TDM.DataModuleCreate(Sender: TObject);
begin
  if Conexao.Connected = True then
     Conexao.Connected := False;
  if (not Conexao.Connected ) then
  begin
    try
      IniControle := TIniFile.Create(ExtractFilePath(Application.ExeName)+'PAF_ECF.INI');
      if Trim(IniControle.ReadString('SERVIDOR', 'DADOS', '')) <> '' then
      begin
        Conexao.Params.Clear;
        Conexao.Params.Values['DATABASE'] := Inicontrole.ReadString('SERVIDOR','DADOS','');
        Conexao.Params.Values['User_Name'] := Inicontrole.ReadString('SERVIDOR','User_Name','');
        Conexao.Params.Values['PASSWORD'] := Inicontrole.ReadString('SERVIDOR','PASSWORD','masterkey');
        Conexao.Params.Values['SQLDialect'] := Trim(Inicontrole.ReadString('SERVIDOR','SQLDialect','3'));
        CAIXA :=Inicontrole.ReadString('SERVIDOR','CAIXA','');
        LOJA :=Inicontrole.ReadString('SERVIDOR','LOJA','');
        IMPRESSORA :=Inicontrole.ReadString('SERVIDOR','IMPRESSORA','');
        LOGO := IniControle.ReadString('SERVIDOR','LOGO','');
        EXPORTA := IniControle.ReadString('SERVIDOR','EXPORTA','');
        TABELA_PRECO := IniControle.ReadString('SERVIDOR','TABELA_PRECO','');
        VENDEDOR := IniControle.ReadString('SERVIDOR','VENDEDOR','');
        CONFIGURADO := IniControle.ReadString('SERVIDOR','CONFIGURADO','');
        BALANCA    := IniControle.ReadString('SERVIDOR','BALANCA','');
        Conexao.connected:= true;
        Inicontrole.Free;
      end;
    except
      Application.MessageBox( 'No foi possivel conectar com o banco de dados.' + #13 +
                              'verifique se o arquivo de configurao est configurado, ' + #13 +
                              'Tente Novamente ou Entre em contato com suporte para esclarecer qualquer dvida.',
                              'Error Interno',mb_ok + mb_IconError );
        Application.Terminate;
    end;
  end;

end;

procedure TDM.dspClienteGetTableName(Sender: TObject; DataSet: TDataSet;
  var TableName: string);
begin
  TableName := 'TB_CLIENTE';
end;


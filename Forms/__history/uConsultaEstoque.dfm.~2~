  object qConsultaEstoque: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'SELECT'
      ' GP.DESCRICAO AS GRUPO,'
      ' p.CODIGO,'
      ' p.CD_BARRA,'
      ' p.DESCRICAO,'
      ' P.UNIDADE,'
      ' P.EMBALGEM,'
      ' P.ESTOQUE,'
      ' P.VL_CUSTO,'
      '  (select SUM(N.QUANTIDADE) FROM TB_ITEM_ENTRADA_NF N'
      '   WHERE N.ID_GRUPO_PRODUTO = P.ID_GRUPO_PRODUTO'
      '   AND N.ID_PRODUTO = P.ID_PRODUTO) AS ENTRADA,'
      '  (select SUM(I.QTDVENDA) FROM TB_ITEMVENDADTO I'
      '   WHERE I.ID_GRUPO_PRODUTO = P.ID_GRUPO_PRODUTO'
      '   AND I.ID_PRODUTO = P.ID_PRODUTO) as saidas,'
      ''
      '   (select SUM(N.QUANTIDADE) FROM TB_ITEM_ENTRADA_NF N'
      '   WHERE N.ID_GRUPO_PRODUTO = P.ID_GRUPO_PRODUTO'
      '   AND N.ID_PRODUTO = P.ID_PRODUTO)-'
      '   (select SUM(I.QTDVENDA) FROM TB_ITEMVENDADTO I'
      '   WHERE I.ID_GRUPO_PRODUTO = P.ID_GRUPO_PRODUTO'
      '   AND I.ID_PRODUTO = P.ID_PRODUTO) as saldo,'
      '   P.ESTOQUE * P.VL_CUSTO AS TOTAL'
      'FROM TB_PRODUTO P'
      
        'LEFT OUTER JOIN TB_GRUPO_PRODUTO GP ON (GP.ID_GRUPO_PRODUTO = P.' +
        'ID_GRUPO_PRODUTO)'
      'ORDER BY P.DESCRICAO')
    SQLConnection = DM.conexao
    Left = 563
    Top = 8
    object qConsultaEstoqueGRUPO: TStringField
      FieldName = 'GRUPO'
      Size = 30
    end

    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'SELECT'
      ' NQ.id_nota_quebra,'
      ' nq.id_tingimento,'
      ' nq.id_for_tingimento,'
      ' nq.id_for_tint,'
      ' nq.id_cliente,'
      ' nq.quant,'
      ' nq.valor,'
      ' nq.nr_nota_quebra'
      'FROM tb_nota_quebra NQ'
      'JOIN tb_tingimento T ON (T.id_tingimento = NQ.id_tingimento)'
      'WHERE T.id_cliente = 3'
      'AND T.id_forn_tint = 7'
      'AND NQ.quant <> 18')

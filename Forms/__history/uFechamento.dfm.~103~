  object qNotaQuebra: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'SELECT'
      'Q.id_nota_quebra,'
      'q.id_tingimento,'
      'q.id_for_tingimento,'
      'q.id_for_tint,'
      'q.id_cliente,'
      'q.quant,'
      'q.valor,'
      'q.nr_nota_quebra,'
      't.dt_emissao,'
      'F.nome AS MALHARIA,'
      'FF.nome AS TITURARIA'
      'FROM tb_nota_quebra Q'
      'join tb_fornecedor F ON (F.id_fornecedor = Q.id_for_tingimento)'
      'join tb_fornecedor FF ON (FF.id_fornecedor = Q.id_for_tint)'
      'JOIN tb_tingimento T ON (T.id_tingimento = Q.id_tingimento)'
      'WHERE T.id_cliente = 3'
      'AND T.dt_emissao BETWEEN '#39'07/24/2020'#39' AND '#39'08/31/2020'#39)
    SQLConnection = DM.conexao

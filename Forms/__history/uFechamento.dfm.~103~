  object qServicos: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'SELECT'
      ' ST.id_servico_tinturaria,'
      ' st.id_servico,'
      ' st.id_preco_tinturaria,'
      ' st.id_artigo_tinturaria,'
      ' st.id_artigo,'
      ' st.valor,'
      ' s.descricao,'
      ' ('
      '   SELECT IR.quant_faturado FROM tb_item_remessa_producao IR'
      
        '   JOIN tb_remessa_producao RP ON (RP.id_remessa = IR.id_remessa' +
        ')'
      '   JOIN tb_cliente C ON (C.id_cliente = RP.id_cliente)'
      '   JOIN tb_fornecedor F ON (F.id_fornecedor = RP.id_forn_tint)'
      '   WHERE C.id_grupo_empresa = 3'
      '   AND F.id_fornecedor =7'
      ' )AS QUANT,'
      ' ST.valor *  ('
      '   SELECT IR.quant_faturado FROM tb_item_remessa_producao IR'
      
        '   JOIN tb_remessa_producao RP ON (RP.id_remessa = IR.id_remessa' +
        ')'
      '   JOIN tb_cliente C ON (C.id_cliente = RP.id_cliente)'
      '   JOIN tb_fornecedor F ON (F.id_fornecedor = RP.id_forn_tint)'
      '   WHERE C.id_grupo_empresa = 3'
      '   AND F.id_fornecedor =7'
      ' ) AS TOTAL'
      'FROM tb_servico_tinturaria ST'
      'JOIN tb_servico S ON (S.id_servico = ST.id_servico)'
      
        'JOIN tb_preco_tinturaria P ON (P.id_preco_tinturaria = ST.id_pre' +
        'co_tinturaria)'
      
        'JOIN tb_grupo_empresa G ON (G.id_grupo_empresa = P.id_grupo_empr' +
        'esa)'
      'where G.id_grupo_empresa =3'
      'AND P.id_fornecedor = 7')
    SQLConnection = DM.conexao
    Left = 496
    Top = 50
    object qServicosID_SERVICO_TINTURARIA: TIntegerField
      FieldName = 'ID_SERVICO_TINTURARIA'
      Required = True

  object dspFaturamento: TDataSetProvider
    DataSet = qFaturamento
    Options = [poIncFieldProps, poAllowMultiRecordUpdates, poAutoRefresh, poAllowCommandText, poUseQuoteChar]
    Left = 208
    Top = 139
  end
  object qFaturamento: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'SELECT'
      ' '#39'FIO'#39','
      ' p.nr_nota_fiscal,'
      ' p.dt_emissao,'
      ' p.vl_nota,'
      ' F.nome AS FORNECEDOR'
      'FROM tb_producao p'
      'JOIN tb_fornecedor F ON (F.id_fornecedor = P.id_forncedor)'
      'WHERE P.id_cliente = 3'
      'AND P.dt_emissao between '#39'07/24/2020'#39' and '#39'08/31/2020'#39
      'union'
      'SELECT'
      ' '#39'MALHARIA'#39','
      ' rp.nr_nota_fiscal,'
      ' rp.dt_emissao,'
      ' rp.vl_nota,'
      ' F.nome AS FORNECEDOR'
      'FROM tb_remessa_producao Rp'
      'JOIN tb_fornecedor F ON (F.id_fornecedor = RP.id_for_remessa)'
      'WHERE RP.id_cliente = 3'
      'AND RP.dt_emissao between '#39'07/24/2020'#39' and '#39'08/31/2020'#39
      'union'
      'SELECT'
      ' '#39'TINTURARIA'#39','
      ' t.nr_nota_fiscal,'
      ' t.dt_emissao,'
      ' t.vl_nota,'
      ' F.nome AS FORNECEDOR'
      'FROM tb_tingimento t'
      'JOIN tb_fornecedor F ON (F.id_fornecedor = t.id_for_tingimento)'
      'WHERE t.id_cliente = 3'
